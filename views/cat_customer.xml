<odoo>
  <data>
    <!-- category.customer form view -->
    <record id="customer_view_form" model="ir.ui.view">
      <field name="name">Instansi/Lembaga/Perseroan</field>
      <field name="model">category.customer</field>
      <field name="arch" type="xml">
        <form string="">
          <sheet>
            <group>
              <!-- Add your fields here -->
              <field name="name"/>
              <field name="parent_category"/>
              <field name="desc"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- category.customer tree view -->
    <record id="category_customer_view_tree" model="ir.ui.view">
      <field name="name">Instansi/Lembaga/Perseroan</field>
      <field name="model">category.customer</field>
      <field name="arch" type="xml">
        <tree>
          <!-- Add your fields here -->
          <field name="complete_name"/>
          <field name="desc"/>
        </tree>
      </field>
    </record>
   
    <!-- category.customer action window -->
    <record id="category_customer_action_window" model="ir.actions.act_window">
      <field name="name">Instansi/Lembaga/Perseroan</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">category.customer</field>
      <field name="view_mode">tree,form</field>
      <field name="view_type">form</field>
      <field name="help" type="html">
        <p class="oe_view_nocontent_create">
          <!-- Add Text Here -->
        </p><p>
          <!-- More details about what a user can do with this object will be OK --> 
        </p>
      </field>
    </record>
  </data>
</odoo>